{
  "name": "coordenadas-astronomicas",
  "version": "1.0.0",
  "description": "Proyecto de conversor de coordenadas astronomicas usando una arquitectura modelo vista controlador (mvc)",
  "main": "server.js",
  "scripts": {
    "sass": "sass src/sass/styles.sass:src/public/css/styles.css",
    "sass-watch": "sass --watch src/sass/styles.sass:src/public/css/styles.css",
    "start": "concurrently \"npm run sass-watch\" \"nodemon src/server/server.js\""
  },
  "repository": {
    "type": "git",
