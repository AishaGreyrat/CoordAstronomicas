  "bugs": {
    "url": "https://github.com/AishaGreyrat/CoordAstronomicas/issues"
  },
  "homepage": "https://github.com/AishaGreyrat/CoordAstronomicas#readme",
  "devDependencies": {
    "concurrently": "^9.2.0",
    "dotenv": "^16.5.0",
    "express": "^5.1.0",
    "nodemon": "^3.1.10",
    "pug": "^3.0.3"
  }
}
